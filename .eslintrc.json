{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      },    "severity": "error"
    
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["only-warn"],
    "rules": {
        "no-unused-vars": "error",
        "no-console": "error",
        "no-unexpected-multiline":"error",
        "babel-eslint": "off",
        "quotes": ["error", "double"],
        "no-multiple-empty-lines": ["error", { "max": 1 }]
     }
     
}
