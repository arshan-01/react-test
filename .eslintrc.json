{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      },
     
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["only-warn"],
    "rules": {
        "no-unused-vars": "warns",
        "no-console": "warns",
        "no-unexpected-multiline": "warns",
        "babel-eslint": "off",
        "quotes": ["warns", "double"],
        "no-multiple-empty-lines": ["warns", { "max": 1 }]
    },
    "severity": "warn"
}
