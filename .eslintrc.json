{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["only-warn"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-unexpected-multiline": "warn",
    "babel-eslint": "off",
    "quotes": ["warn", "double"],
    "no-multiple-empty-lines": ["warn", { "max": 1 }]
  }
}
