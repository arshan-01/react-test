{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }

    },
    "parser": "@typescript-eslint/parser",
    "rules": {
        "no-unused-vars": 1,
        "no-console": 1,
        "no-unexpected-multiline": 1,
        "babel-eslint": "off",
        "quotes": [1, "double"],
        "no-multiple-empty-lines": [1, { "max": 1 }]
     }
     
}
